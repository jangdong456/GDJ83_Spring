<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jang.app.notice.NoticeDAO">
	
	<select id="list" parameterType="NoticeDTO" resultMap="noticeList">
		SELECT * 
		FROM
			NOTICE
			INNER JOIN
			MEMBERS
			ON NOTICE.BOARD_WRITER = MEMBERS.M_ID
		WHERE MEMBERS.M_ID = #{m_id}
	</select>
	
	
	<!--  누가 1이고 누가 N인지 살펴보기  -->
	
	<resultMap type="NoticeDTO" id="noticeList">
		<id column="BOARD_NUM" property="board_num"/>
		<result />
		
		
		<!-- N -->
		<collection property="">
		
		</collection>
	
	</resultMap>
	
	<insert id="add">
		INSERT INTO NOTICE
		VALUES (BOARD_SEQ.NEXTVAL,10,'테스트','내용 아무거나 테스트',SYSDATE, SYSDATE,'1',0);
	</insert>
	
</mapper>